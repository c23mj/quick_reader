{
  "name": "Quick Reader",
  "description": "A simple chrome extension to help you read more efficiently!",
  "author": "Michael Jiang",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "assets/icons/qr16.png",
    "32": "assets/icons/qr32.png",
    "48": "assets/icons/qr48.png",
    "128": "assets/icons/qr128.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "assets/popup/popup.html"
  },
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  }
}
